{
  "name": "pawnrs",
  "module": "scripts/mod.ts",
  "type": "module",
  "scripts": {
    "build": "cargo build --workspace",
    "tests": "bun scripts/commands/test-examples.ts",
    "rustpwn": "cargo run --quiet --bin pawnc --",
    "rs:check": "bun rustpwn --check",
    "rs:fix": "bun rustpwn --fix",
    "check": "tsc --noEmit && bun check --write --unsafe"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/bun": "^1.2.21",
    "typescript": "^5.9.2",
    "ultracite": "^5.2.17"
  }
}